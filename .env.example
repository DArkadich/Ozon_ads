# Ozon API credentials (обязательно)
OZON_CLIENT_ID=your_client_id_here
OZON_API_KEY=your_api_key_here

# Telegram Bot (опционально)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_chat_id_here

# External services (опционально)
MPSTATS_API_KEY=your_mpstats_key_here
KEYS_SO_API_KEY=your_keys_so_key_here

# Настройки приложения
LOG_LEVEL=INFO
REPORT_OUTPUT_DIR=./reports
AUTO_OPTIMIZATION_ENABLED=false

# База данных (если используете PostgreSQL профиль)
POSTGRES_PASSWORD=your_secure_password_here
